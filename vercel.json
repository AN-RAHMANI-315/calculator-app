{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "public/api/*.py": {
      "runtime": "python3.9"
    }
  },
  "env": {
    "FLASK_ENV": "production"
  }
}
